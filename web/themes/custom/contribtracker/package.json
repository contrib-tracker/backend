{
  "scripts": {
    "build": "gulp",
    "lint": "CI=true gulp lint",
    "vr": "percy exec -- cypress run",
    "vr-prod": "CYPRESS_VIDEO=false percy exec -- cypress run --env version=prod --spec cypress/e2e/VR_tasks/*.cy.js",
    "vr-dev": "CYPRESS_VIDEO=false percy exec -- cypress run --env version=dev --spec cypress/e2e/VR_tasks/*.cy.js",
    "functional-prod": "cypress run --env version=prod --spec cypress/e2e/Functional_tasks/*.cy.js",
    "functional-dev": "cypress run --env version=dev --spec cypress/e2e/Functional_tasks/*.cy.js",
    "cypress-open": "cypress open --env version=prod --browser chrome",
    "cypress-run": "cypress run --env version=prod --browser chrome",
    "cypress:run:without-video": "CYPRESS_VIDEO=false cypress run --env version=prod"
  },
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "@percy/cli": "^1.29.0",
    "@percy/cypress": "^3.1.2",
    "@ronilaukkarinen/gulp-stylelint": "^14.1.2",
    "@types/gulp": "^4.0.17",
    "@types/gulp-sass": "^5.0.4",
    "@types/jquery": "^3.5.31",
    "@types/jqueryui": "^1.12.23",
    "@types/node": "^22.7.5",
    "@types/select2": "^4.0.63",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.20",
    "breakpoint-sass": "^3.0.0",
    "browser-sync": "^3.0.2",
    "cypress": "13.12.0",
    "cypress-real-events": "^1.13.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gulp": "^5.0.0",
    "gulp-eslint-new": "^2.3.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^9.1.0",
    "gulp-postcss": "^10.0.0",
    "gulp-prettier": "^6.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "postcss": "^8.4.40",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.1",
    "sass": "^1.77.8",
    "sass-toolkit": "^2.10.2",
    "stylelint": "^15.11.0",
    "stylelint-prettier": "^4.1.0",
    "stylelint-scss": "5.0",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0"
  },
  "dependencies": {
    "select2": "^4.1.0-rc.0"
  },
  "overrides": {
    "minimatch": "5.1.2",
    "glob": "8.1.0"
  },
  "version": "1.0.0",
  "type": "module"
}
